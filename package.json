{
  "name": "ReactNativeTS",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "format": "prettier --write '{.,__tests__,src/**}/*.{js,jsx,ts,tsx}'",
    "lint": "tslint -p .",
    "precommit": "tsc && lint-staged",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "studio": "open -a /Applications/Android\\ Studio.app ./android/",
    "test": "jest",
    "tsc": "tsc -p .",
    "xcode": "open ios/$npm_package_name.xcodeproj"
  },
  "dependencies": {
    "apisauce": "^0.15.2",
    "immutable": "^4.0.0-rc.9",
    "react": "16.4.1",
    "react-native": "0.56.0",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@types/jest": "^23.3.1",
    "@types/react": "^16.4.7",
    "@types/react-native": "^0.56.4",
    "@types/react-redux": "^6.0.5",
    "@types/react-test-renderer": "^16.0.1",
    "@types/webpack-env": "^1.13.6",
    "babel-jest": "23.4.2",
    "babel-preset-react-native": "^5",
    "husky": "^0.14.3",
    "jest": "23.4.2",
    "lint-staged": "^7.2.0",
    "prettier": "^1.13.7",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.4.1",
    "redux-devtools-extension": "^2.13.5",
    "remote-redux-devtools": "^0.5.12",
    "ts-jest": "^23.0.1",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.9.2",
    "tslint-config-prettier": "^1.14.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.0.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
